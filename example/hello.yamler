enum Status {
    SUCCESS = success
    ERROR = failure
}

ruleset address {
    road str required
}

ruleset person {
    hello str
    name str
    surname str
    age int
    address (address)
}

ruleset main {
    number int
    root (person) required
    person (person) optional
    my_map map(int) required
    numbers list((address))
    enum1 Status required
}