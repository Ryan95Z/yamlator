enum Status {
    CREATED = "created"
    ACTIVE = "active"
    DELETED = "deleted"
}

ruleset Details {
    owner str
    team str
    labels map(str)
}

ruleset Project {
    version str
    projectId str
    displayName str
    iam_permission list(regex("^role{1}[0-9]{1}"))
    labels map(str)
    status Status
    project_details Details
}

schema {
    project Project
    test union(int, str, list(int), list(list(int)))
}
