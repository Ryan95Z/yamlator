enum Status {
    CREATED = created
    ACTIVE = active
    DELETED = deleted
}

ruleset Details {
    owner str
    team str
    labels map(str)
}

ruleset Project {
    version str
    projectId str
    displayName str
    iam_permission list(str)
    labels map(str)
    status Status
    project_details Details
}

schema {
    project Project
    test regex("roles/*")
}
